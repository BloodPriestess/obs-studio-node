{
  "compileOnSave": true,
  "compilerOptions": {
    "outDir": "./distribute",
    "declaration": true,
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "isolatedModules": false,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "removeComments": true,
    "suppressImplicitAnyIndexErrors": true,
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": false,
    "emitDecoratorMetadata": true
  },
  "exclude": [
	"build",
	"distribute",
	"logs",
	"node_modules",
	"obs-prebuild",
	"resources",
	"src",
  ],
  "files": [
    "js/index.ts",
    "js/module.ts",
    "js/type_check.ts"
  ]
}